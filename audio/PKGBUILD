groups=(eagebhart)

# audio
# alsa - is a group. See Makefile.
depends=(pulseaudio pulseaudio-alsa pulseaudio-bluetooth cdparanoia lame)
# audio players
depends+=(mpd mpc)
# audio taggers
depends+=(picard kid3)
# video players
depends+=(mplayer vlc)

rootdir=$PWD
